    <script>

        call();
        var myVar = setInterval(call, 5000);
        function call(){
            $.ajax({
                type: "POST",
                url: "./check-file",
                data: "",
                cache: false,
                success: function (msg) {
                    //alert(msg);
                    data = msg.detail;
                    var append = '\n';
                    for(var i=0;i<data.length;i++){
                        append += '<a class="booking-item" href="#">\n';
                        append += ' <div class="row" >\n';

                        var j=0;
                        /*console.log(dataall[i].length);*/
                        $.each(data[i]._data, function(key, value) {
                            if(j==0){
                                append+='<div class="col-md-2">\n';
                            }else if(j==1){
                                append+='<div class="col-md-4">\n';
                            }else if(j==2){
                                append+='<div class="col-md-2">\n';
                            }else {
                                append+='<div class="col-md-4">\n';
                            }

                            if(typeof  value[1]._data === 'object'){
                                append += ' <div class="row" >\n';
                                append+= '<h5 class="booking-item-title" style="color:red ">'+key+'</h5>\n';
                                $.each(value[1]._data, function(key1, value1) {

                                    append+= '<p class="booking-item-title">'+value1[0]+ ' : ' +value1[1]+'</p>\n';
                                });
                                append+='</div>\n';
                                j++;
                            }else{
                                append += ' <div class="row" >\n';
                                append+= '<h5 class="booking-item-title" style="color:red ">'+value[0]+ '</h5>\n';
                                append+='</div>\n';
                                append += ' <div class="row" >\n';
                                append+= '<p class="booking-item-title">'+value[1]+'</p>\n';
                                append+='</div>\n';
                                j++;
                            }
                            append+='</div>\n';
                        });

                        append+='</div>\n';
                        append+='</a>\n';
                    }




                    /*result = datas.names;*/

                    $('#standard').after(append);


                }


            });
        }


    </script>
